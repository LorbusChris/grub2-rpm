---
 util/setup.c | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/util/setup.c b/util/setup.c
index b299159..8f20e99 100644
--- a/util/setup.c
+++ b/util/setup.c
@@ -529,7 +529,7 @@ SETUP (const char *dir,
     bl.block = bl.first_block;
     while (bl.block->len)
       {
-	grub_memset (bl.block, 0, sizeof (bl.block));
+	grub_memset (bl.block, 0, sizeof (*bl.block));
       
 	bl.block--;
 
